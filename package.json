{
	"name": "vue-imgs-previewer",
	"version": "0.1.0",
	"private": false,
	"main": "dist/vue-image-preview.umd.js",
	"module": "src/index.js",
	"scripts": {
		"serve-demo": "vue-cli-service serve --mode demo --copy",
		"build-demo": "vue-cli-service build --mode demo",
		"build-lib": "vue-cli-service build --mode lib --target lib --name vue-image-preview src/index.js",
		"lint": "vue-cli-service lint"
	},
	"dependencies": {
		"core-js": "^3.3.2",
		"swiper": "^5.2.0",
		"vue": "^2.6.10",
		"vue-router": "^3.1.3"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^4.0.0",
		"@vue/cli-plugin-eslint": "^4.0.0",
		"@vue/cli-plugin-router": "^4.0.0",
		"@vue/cli-service": "^4.0.0",
		"@vue/eslint-config-prettier": "^5.0.0",
		"babel-eslint": "^10.0.3",
		"eslint": "^5.16.0",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-vue": "^5.0.0",
		"less": "^3.0.4",
		"less-loader": "^5.0.0",
		"lint-staged": "^9.4.2",
		"prettier": "^1.18.2",
		"vue-template-compiler": "^2.6.10"
	},
	"gitHooks": {
		"pre-commit": "lint-staged"
	},
	"browserslist": [
		"last 2 version",
		"> 1%",
		"not dead"
	],
	"prettier": {
		"tabWidth": 4,
		"useTabs": true,
		"jsxBracketSameLine": false,
		"printWidth": 460,
		"proseWrap": "never"
	},
	"lint-staged": {
		"*.{js,vue}": [
			"vue-cli-service lint",
			"git add"
		]
	}
}
